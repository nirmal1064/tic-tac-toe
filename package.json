{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "workspaces": [
    "client",
    "server",
    "utils"
  ],
  "scripts": {
    "client": "yarn workspace @tic-tac-toe/client",
    "server": "yarn workspace @tic-tac-toe/server",
